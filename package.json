{
  "name": "react-material-theme-provider",
  "version": "0.1.2",
  "license": "Apache-2.0",
  "description": "Material Design 3 theme token generator for React applications",
  "keywords": [
    "material-design",
    "material-you",
    "theme",
    "react",
    "tokens",
    "css-variables"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zZHorizonZz/react-material-theme-provider.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist .jest-cache",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --detectOpenHandles --forceExit",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch --detectOpenHandles --forceExit",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --detectOpenHandles --forceExit",
    "lint": "biome lint --unsafe ./src",
    "lint:fix": "biome lint --write --unsafe ./src",
    "format": "biome format --write ./src"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "@material/material-color-utilities": "^0.3.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@biomejs/biome": "1.9.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/jest": "^29.5.14",
    "@jest/globals": "^29.7.0",
    "typescript": "^5.6.3",
    "tsup": "^8.3.5",
    "ts-node": "^10.9.2",
    "rimraf": "^6.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5"
  }
}
